<?xml version="1.0" encoding="utf-8"?>
<preset name="android-x86" comment="Android-19, x86 PhysX SDK ">
  <platform targetPlatform="android" compiler="clang" />
  <CMakeSwitches>
    <cmakeSwitch name="PX_BUILDSNIPPETS" value="False" comment="Generate the snippets" />
    <cmakeSwitch name="PX_GENERATE_STATIC_LIBRARIES" value="True" comment="Generate static libs" />
  </CMakeSwitches>
  <CMakeParams>
    <cmakeParam name="CMAKE_INSTALL_PREFIX" value="install/android-x86/PhysX" comment="Install path relative to PhysX SDK root" />
    <cmakeParam name="ANDROID_NATIVE_API_LEVEL" value="android-19" comment="Android platform API level" />
    <cmakeParam name="ANDROID_ABI" value="x86" comment="Android arm ABI" />
  </CMakeParams>
</preset>
